procedura
{
    zacznij program();
    jeśli(1) { wypisz("JESTEM W JEŚLI"); }
    całk a;
    a=1;
    dopóki(a<=10)
    {
     wypisz("JESTEM W DOPÓKI");
     wypisz(a);
     jeśli(a<3){wypisz("ALE TO JESZCZE DALEKO\n");}
     a=a+p(1);
     kontynuuj;
    }

    zwróć();
}
