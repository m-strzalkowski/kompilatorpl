procedura -> całk
{
    całk odp;
    zacznij ack(całk m, całk n);
    jeśli(m==0){odp = n+1;}
    inaczej jeśli(n==0){odp = ack(m-1, 1);}
    inaczej{odp = ack(m-1, ack(m,n-1));}
    zwróć(odp);
}

procedura
{
    całk i; całk j;
    zacznij program();
    i=0;
    dopóki(i<3)
    {
        j=0;
        dopóki(j<3)
        {

            ack(i,j);
            //piszf("Wartość funkcji Ackermana(%d, %d) to %d\n", i,j,ack(i,j));
            j+=1;
        }
        i+=1;
    }
}

